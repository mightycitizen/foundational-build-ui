<ul class="list-none flex {{ display == 'inline' ? 'flex-wrap' : 'flex-col' }} gap-2.5">
  {% for link in links %}
    <li>
      {% if arrow | default %}
        {% include "atoms:read-more" with link %}
      {% else %}
        {% include "atoms:link" with link %}
      {% endif %}
    </li>
  {% endfor %}
</ul>

{% extends '@wrappers/default.twig' %}
{% block top %}
  {% include '@components/containers/hero/hero.twig' with hero %}
{% endblock %}
{% block main %}

  {% for module in modules %}
    {% if module.module_type == 'cta' %}
      {% include '@components/modules/cta/cta.twig' with module only %}
    {% elseif module.module_type == 'cardgroup' %}
      {% include '@components/modules/card-group/card-group-module.twig' with module only %}
    {% endif %}
  {% endfor %}

{% endblock %}

